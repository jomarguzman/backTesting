    "properties": {
      "postId": {
        "$id": "#/items/properties/postId",
        "type": "integer",
        "title": "The Postid Schema"
      },
      "id": {
        "$id": "#/items/properties/id",
        "type": "integer",
        "title": "The Id Schema"
      },
      "name": {
        "$id": "#/items/properties/name",
        "type": "string",
        "title": "The Name Schema"
      },
      "email": {
        "$id": "#/items/properties/email",
        "type": "string",
        "title": "The Email Schema"
      },
      "body": {
        "$id": "#/items/properties/body",
        "type": "string",
        "title": "The Body Schema"
